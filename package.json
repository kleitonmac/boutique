{
  "name": "alana-boutique",
  "version": "1.0.0",
  "description": "E-commerce de moda feminina",
  "main": "server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd front/core && npm run dev",
    "build": "cd front/core && npm run build",
    "install-all": "npm install && cd front/core && npm install",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "keywords": ["e-commerce", "moda", "boutique", "react", "node"],
  "author": "Alana Boutique",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1"
  }
}
